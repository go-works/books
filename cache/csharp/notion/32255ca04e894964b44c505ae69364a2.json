          "title": [
            [
              "public virtual bool Set\u003cT\u003e(ref T field, T value, [CallerMemberName] string propertyName = null)\n    {\n        if (Equals(field, value))\n            return false;\n        storage = value;\n        RaisePropertyChanged(propertyName);\n        return true;\n    }\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "public virtual bool Set\u003cT\u003e(ref T field, T value, [CallerMemberName] string propertyName = null)\n    {\n        if (Equals(field, value))\n            return false;\n        storage = value;\n        RaisePropertyChanged(propertyName);\n        return true;\n    }\n}",
        "code_language": "Plain Text"
