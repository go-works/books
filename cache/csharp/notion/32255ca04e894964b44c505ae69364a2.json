          "title": [
            [
              "public class NotifyPropertyChangedBase : INotifyPropertyChanged\n{\n    protected void RaisePropertyChanged([CallerMemberName] string propertyName = null) =\u003e\n    PropertyChanged?.Invoke(this, new PropertyChangedEventArgs(propertyName));\n    \n    public event PropertyChangedEventHandler PropertyChanged;"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "public class NotifyPropertyChangedBase : INotifyPropertyChanged\n{\n    protected void RaisePropertyChanged([CallerMemberName] string propertyName = null) =\u003e\n    PropertyChanged?.Invoke(this, new PropertyChangedEventArgs(propertyName));\n    \n    public event PropertyChangedEventHandler PropertyChanged;",
        "code_language": "Plain Text"
