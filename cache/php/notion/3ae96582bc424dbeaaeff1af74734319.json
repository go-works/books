              "$array = [\"alpha\", \"beta\", \"gamma\", \"delta\", \"epsilon\"];\nfor ($i = 0; $i \u003c count($array); $i++) {\n    echo $array[$i], PHP_EOL;\n    if ($array[$i] === \"gamma\") {\n        $array[$i] = \"zeta\";\n        $i -= 2;\n    } elseif ($array[$i] === \"zeta\") {\n        $i++;\n    }\n}"
